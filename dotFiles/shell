export EDITOR=nano
export VISUAL=nano

alias showGpu='cat /sys/class/drm/card1/device/pp_od_clk_voltage'
alias makesrcinfo='makepkg --printsrcinfo > .SRCINFO'
alias editPacman='sudo nano /etc/pacman.conf'
alias editMakepkg='sudo nano /etc/makepkg.conf'
alias pmIsInst='pacman -Qs'
alias pmUninst='sudo pacman -Rsn'
alias pmInst='sudo pacman -S'
alias removeExif='exiftool -overwrite_original -all='
alias grubup="sudo grub-mkconfig -o /boot/grub/grub.cfg"
alias ls='ls -lah --color'
